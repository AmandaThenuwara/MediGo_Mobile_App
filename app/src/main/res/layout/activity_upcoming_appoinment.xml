                    <ImageView
                        android:id="@+id/imageView108"
                        android:layout_width="54dp"
                        android:layout_height="18dp"
                        android:layout_gravity="center|right|top"
                        android:layout_marginStart="44dp"
                        android:layout_marginTop="4dp"
                        app:layout_constraintStart_toEndOf="@+id/imageView106"
                        app:layout_constraintTop_toBottomOf="@+id/imageView112"
                        app:srcCompat="@drawable/rectangle_white" />

                    <TextView
                        android:id="@+id/textView50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|right|top"
                        android:layout_marginBottom="4dp"
                        android:fontFamily="@font/league_spartan"
                        android:text="4.30pm"
                        android:textColor="@color/secondary"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/imageView108"
                        app:layout_constraintEnd_toEndOf="@+id/imageView108"
                        app:layout_constraintStart_toEndOf="@+id/imageView109" />

                    <ImageView
                        android:id="@+id/imageView109"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_gravity="center|right|top"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="3dp"
                        app:layout_constraintStart_toStartOf="@+id/imageView108"
                        app:layout_constraintTop_toTopOf="@+id/imageView108"
                        app:srcCompat="@drawable/baseline_access_alarm_24" />

                    <ImageView
                        android:id="@+id/imageView106"
                        android:layout_width="92dp"
                        android:layout_height="18dp"
                        android:layout_gravity="center|right|top"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="4dp"
                        app:layout_constraintStart_toEndOf="@+id/cardView"
                        app:layout_constraintTop_toBottomOf="@+id/imageView112"
                        app:srcCompat="@drawable/rectangle_white" />

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_gravity="center|right|top"
                        android:layout_marginTop="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/textView48"
                        app:layout_constraintHorizontal_bias="0.44"
                        app:layout_constraintStart_toStartOf="@+id/imageView106"
                        app:layout_constraintTop_toTopOf="@+id/imageView106"
